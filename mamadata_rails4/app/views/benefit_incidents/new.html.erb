					<%= render 'form' %>
				<% else %>
					<h2>&nbsp;</h2>
					<h4 class="text-center">Please add person to a program</h4>
				<% end %>
			</div>
			<div class="text-center">
				<%= link_to 'Cancel', person_path(@benefit_incident.person_id), :class => "button tiny" %>
			</div>
		</div>
	</div>
</div>